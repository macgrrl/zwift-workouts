        <SteadyState Duration="120" Power="0.53"/>
        <SteadyState Duration="180" Power="0.86"/>
        <SteadyState Duration="180" Power="0.89"/>
        <SteadyState Duration="180" Power="0.53"/>
        <SteadyState Duration="180" Power="0.89"/>
        <SteadyState Duration="180" Power="0.86"/>
        <SteadyState Duration="180" Power="0.53"/>
        <SteadyState Duration="180" Power="0.86"/>
        <SteadyState Duration="180" Power="0.89"/>
        <SteadyState Duration="180" Power="0.53"/>
        <IntervalsT Repeat="4" OnDuration="60" OffDuration="180" OnPower="1" OffPower="0.53"/>
        <Cooldown Duration="180" PowerLow="0.53" PowerHigh="0.4"/>
    </workout>
</workout_file>
